
type label = string

type valid = bool

type labeled =
  { flag: valid
  ; lb: label
  ; count: int
  }

type point = (int * int * string * unit)

type 'a param =
  { data: 'a
  ; nothing: unit
  }

type 'a same_pair = ('a * 'a)

type ('a, 'b) pair =
  { left: 'a
  ; right: 'b
  }


type 'a pairs = ('a, 'a) pair list